@font-face {
  font-family: 'fontello';
  src: url('<%= font_url("fontello.eot") %>');
  src: url('<%= font_url("fontello.eot#iefix") %>') format('embedded-opentype'),
       url('<%= font_url("fontello.woff") %>') format('woff'),
       url('<%= font_url("fontello.ttf") %>') format('truetype'),
       url('<%= font_url("fontello.svg#fontello") %>') format('svg');
  font-weight: normal;
  font-style: normal;
}
[class^="icon-"]:before, [class*=" icon-"]:before {
  font-family: "fontello";
  font-style: normal;
  font-weight: normal;
  speak: none;

  display: inline-block;
  text-decoration: inherit;
  width: 1em;
  margin-right: .2em;
  text-align: center;

  /* For safety - reset parent styles, that can break glyph codes*/
  font-variant: normal;
  text-transform: none;

  /* fix buttons height, for twitter bootstrap */
  line-height: 1em;

  /* Animation center compensation - margins should be symmetric */
  /* remove if not needed */
  margin-left: .2em;
}

.icon-heart:before { content: '\e804'; } /* '' */
.icon-heart-empty:before { content: '\e801'; } /* '' */
.icon-user:before { content: '\e802'; } /* '' */
.icon-facebook:before { content: '\e806'; } /* '' */
.icon-twitter:before { content: '\e807'; } /* '' */
.icon-export:before { content: '\e803'; } /* '' */
.icon-trash:before { content: '\e805'; } /* '' */
.icon-rss:before { content: '\e800'; } /* '' */
