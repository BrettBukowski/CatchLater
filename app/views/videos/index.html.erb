<%= render partial: "heading", locals: { 
			heading: "Your list",
			videos: @videos,
			emptyMessage: "There's nothing here"
} %>
<nav>
	<%= link_to "Favorites", faves_videos_path %>
</nav>
<% @tags.each do |tag| %>
<%= tag['_id'] %> (<%= tag['value'].to_i %>)<br>
<% end %>
<% if notice %>
<div id="notice"><%= notice %></div>
<% end %>
<section class="queue videoList">
	<%= render @videos %>
</section>