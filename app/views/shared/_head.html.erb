<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <meta name="description" content="Save online videos across the web for later viewing">
  <meta id="viewport" name="viewport" content="initial-scale=1.0,user-scalable=no,minimum-scale=1.0,maximum-scale=1.0">
  <meta name="apple-mobile-web-app-capable" content="yes">
	<link href='/favicon.ico' rel='icon' type='image/x-icon'>
	<title>CatchLater</title>
	<%= stylesheet_link_tag 'application' %>
  <link href="//fonts.googleapis.com/css?family=Molengo:regular|News+Cycle|Sue+Ellen+Francisco" rel="stylesheet" type="text/css">
  <%= csrf_meta_tag %>
  <% if controller.controller_name == 'videos' && current_user %>
  <%= auto_discovery_link_tag :atom, '/videos/feed.atom?key=' + current_user.feedKey %>
  <% end %>
</head>
